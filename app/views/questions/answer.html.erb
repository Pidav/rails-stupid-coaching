<div class="answer-box">
  <div class="talk">
    <p><%= @question %></p>
    <p><em>-You</em></p>
  </div>
  <div class="talk final">
    <p id="answer-phrase"><%= @answer %></p>
    <p><em>-Your Coach</em></p>
  </div>
  <div class="link">
    <%= link_to "Ask a new question", ask_path %>
  </div>
</div>
